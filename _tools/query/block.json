{
	"apiVersion": 2,
	"name": "phenix/px-query",
	"version": "0.1.0",
	"title": "PDS Query",
	"category": "design",
	"description": "Custom Query Posts Block.",
	"supports": {
		"html": false
	},
    "attributes" : {
        "query_type" : {
            "type": "string",
            "default": "native"
        },

        "post_type" : {
            "type": "string",
            "default": "post"
        },

        "per_page" : {
            "type": "number",
            "default": 8
        },

        "order" : {
            "type": "string",
            "default": "DESC"
        },

        "isSlider" : {
            "type": "boolean",
            "default": false
        },

        "pagination" : {
            "type": "boolean",
            "default": false
        }
    },
	"textdomain": "phenix",
	"editorScript": "px-query"
}
