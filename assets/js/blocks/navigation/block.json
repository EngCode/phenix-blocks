{
  "apiVersion": 2,
  "name": "phenix/navigation",
  "version": "0.1.0",
  "title": "PDS Menu",
  "category": "design",
  "description": "",
  "supports": {
    "html": false,
    "align": [
      "full"
    ],
    "alignWide": true,
    "__experimentalOnEnter": true,
    "__experimentalMetadata": true
  },
  "attributes": {
    "preview": {
      "type": "boolean",
      "default": false
    },
    "menu_type": {
      "type": "string",
      "default": "classic"
    },
    "tagName": {
      "type": "string",
      "default": "nav"
    },
    "menu_id": {
      "type": "string",
      "default": ""
    },
    "arrow_icon": {
      "type": "string",
      "default": "far fa-angle-down"
    },
    "responsive": {
      "type": "boolean",
      "default": false
    },
    "mobile_mode": {
      "type": "string",
      "default": "dropdown"
    },
    "effect": {
      "type": "string",
      "default": "slide"
    },
    "direction": {
      "type": "string",
      "default": "vertical"
    },
    "hover": {
      "type": "boolean",
      "default": false
    },
    "typography": {
      "type": "object",
      "default": {}
    },
    "style": {
      "type": "object",
      "default": {}
    },
    "items_icon_op": {
      "type": "boolean",
      "default": false
    },
    "count_badge": {
      "type": "boolean",
      "default": false
    },
    "items_icon": {
      "type": "string",
      "default": "far fa-link"
    },
    "items_count": {
      "type": "string",
      "default": "8"
    },
    "post_type": {
      "type": "string",
      "default": "post"
    }
  },
  "example": {
    "attributes": {
      "preview": true
    }
  },
  "textdomain": "pds-blocks",
  "editorScript": "px-navigation"
}