{
  "apiVersion": 2,
  "name": "phenix/navigation",
  "version": "0.1.0",
  "title": "Phenix Menu",
  "category": "design",
  "description": "Main Navigation Block for Phenix Header.",
  "supports": {
    "html": false
  },
  "attributes": {
    "preview": {
      "type": "boolean",
      "default": false
    },
    "tagName": {
      "type": "string",
      "default": "nav"
    },
    "menu_id": {
      "type": "string",
      "default": ""
    },
    "className": {
      "type": "string",
      "default": ""
    },
    "responsive": {
      "type": "boolean",
      "default": true
    },
    "mobile_mode": {
      "type": "string",
      "default": "dropdown"
    },
    "effect": {
      "type": "string",
      "default": "slide"
    },
    "direction": {
      "type": "string",
      "default": "vertical"
    },
    "hover": {
      "type": "boolean",
      "default": false
    },
    "menus_list": {
      "type": "array",
      "default": []
    }
  },
  "example": {
    "attributes": {
      "preview": true
    }
  },
  "textdomain": "phenix",
  "editorScript": "px-navigation"
}