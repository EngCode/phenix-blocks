{
  "apiVersion": 2,
  "name": "phenix/query",
  "version": "0.1.0",
  "title": "Query Loop",
  "category": "design",
  "description": "Custom Query Posts Block.",
  "supports": {
    "html": false,
    "align": [
      "full"
    ],
    "alignWide": true
  },
  "attributes": {
    "post_type": {
      "type": "string",
      "default": "default"
    },
    "template_part": {
      "type": "string",
      "default": "pds/placeholder"
    },
    "per_page": {
      "type": "number",
      "default": 8
    },
    "order": {
      "type": "string",
      "default": "DESC"
    },
    "pagination": {
      "type": "boolean",
      "default": false
    },
    "preview": {
      "type": "boolean",
      "default": false
    },
    "grid_mode": {
      "type": "boolean",
      "default": false
    },
    "grid": {
      "type": "object",
      "default": {}
    },
    "slider_mode": {
      "type": "boolean",
      "default": false
    },
    "slider": {
      "type": "object",
      "default": {}
    },
    "responsive": {
      "type": "object",
      "default": {}
    }
  },
  "example": {
    "attributes": {
      "preview": true
    }
  },
  "textdomain": "phenix",
  "editorScript": "px-query"
}