    "isFlexbox": {
      "type": "boolean",
      "default": false
    },
    "flexbox": {
      "type": "object",
      "default": {}
    },
    "typography": {
      "type": "object",
      "default": {}
    },
    "style": {
      "type": "object",
      "default": {}
    },
    "responsive": {
      "type": "object",
      "default": {}
    },
    "slider": {
      "type": "object",
      "default": {}
    },
    "preview": {
      "type": "boolean",
      "default": false
    }
  },
  "example": {
    "attributes": {
      "preview": true
    }
  },
  "textdomain": "pds-blocks",
  "editorScript": "pds-taxonomies"
