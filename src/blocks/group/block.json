{
    "apiVersion": 2,
	"name": "phenix/group",
	"version": "0.1.0",
	"title": "PDS Group",
    "keywords": ["pds", "phenix", "group", "wrapper", "container"],
	"category": "design",
	"description": "Flexbox Container to Group the Elements Together from Phenix Blocks.",
	"supports": {
		"html": false
	},
    "attributes" : {
		"id": {
			"type": "string",
			"default": ""
		},

		"tagName": {
			"type": "string",
			"default": "div"
		},

        "preview" : {
            "type": "boolean",
            "default": false
        },

        "isFlexbox" : {
            "type": "boolean",
            "default": true
        },

        "flex_align" : {
            "type": "string",
            "default": ""
        },

        "flexbox": {
            "type": "object",
            "default": {}
        },

        "typography": {
            "type": "object",
            "default": {}
        },

        "style": {
            "type": "object",
            "default": {}
        },

        "responsive": {
            "type": "object",
            "default": {}
        }
    },
    "example": {
        "attributes": {
            "preview": true
        }
    },
	"textdomain": "phenix",
	"editorScript": "file:./index.js"
}