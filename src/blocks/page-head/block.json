{
	"apiVersion": 2,
	"name": "phenix/page-head",
	"version": "0.1.0",
	"title": "Page Head",
	"category": "design",
	"description": "Page Title and Breadcrumb.",
	"supports": {
		"html": false,
        "align": ["full"],
        "alignWide": true,
        "__experimentalOnEnter": true,
		"__experimentalMetadata": true,
		"shadow": true,
        "renaming": true,
        "interactivity": {"clientNavigation": true}
	},

    "attributes" : {
        "metadata": {
            "name": {
                "type": "string",
                "default": ""
            }
        },
		"preview" : {
			"type": "boolean",
			"default": false
		},

		"mode": {
			"type": "string",
			"default": "title-breadcrumb"
		},

		"setting": {
			"type": "object",
			"default": {}
		},

		"typography": {
            "type": "object",
            "default": {}
        },

        "style": {
            "type": "object",
            "default": {}
        },

        "responsive": {
            "type": "object",
            "default": {}
        }
	},
	"example": {
        "attributes": {
            "preview": true
        }
    },
	"textdomain": "pds-blocks",
	"editorScript": "px-head"
}