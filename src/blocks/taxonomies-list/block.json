{
	"apiVersion": 2,
	"name": "phenix/taxonomies-list",
	"version": "0.1.0",
	"title": "Taxonomies List",
	"category": "design",
	"description": "Custom Taxonomies & Categories Query Block.",
	"supports": {
		"html": false
	},
    "attributes" : {
        "taxonomy" : {
            "type": "string",
            "default": "category"
        },

        "post_type" : {
            "type": "string",
            "default": "post"
        },

		"query_count" : {
            "type": "number",
            "default": 0
        },

        "order" : {
            "type": "string",
            "default": "ASC"
        },

		"hide_empty" : {
			"type": "boolean",
			"default": true
		},

		"preview" : {
			"type": "boolean",
			"default": false
		}
    },
	"textdomain": "phenix",
	"editorScript": "taxonomies-list"
}
