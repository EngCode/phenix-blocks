{
	"apiVersion": 2,
	"name": "phenix/theme-part",
	"version": "0.1.0",
	"title": "Theme Part",
	"category": "design",
	"description": "Pull a Dynamic Template Part From Current Theme.",
	"supports": {
		"html": false,
        "align": ["full"],
        "alignWide": true
	},
    "attributes" : {
        "preview" : {
            "type": "boolean",
            "default": false
        },

        "part_name" : {
            "type": "string",
            "default": "_placeholder"
        },

		"part_options" : {
			"type": "object",
			"default": {}
		}
    },

	"example": {
        "attributes": {
            "preview": true
        }
    },
	"textdomain": "pds-blocks",
	"editorScript": "theme-part"
}