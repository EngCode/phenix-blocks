{
	"apiVersion": 2,
	"name": "phenix/theme-part",
	"version": "0.1.0",
	"title": "Theme Part",
	"category": "phenix-blocks",
	"description": "Pull a Dynamic Template Part From Current Theme.",
	"supports": {
		"html": false,
        "renaming": true,
        "align": ["full"],
        "alignWide": true,
        "__experimentalOnEnter": true,
		"__experimentalMetadata": true,
        "interactivity": {"clientNavigation": true}
	},

    "attributes" : {
        "metadata": {
            "type": "object",
            "default": {}
        },

        "preview" : {
            "type": "boolean",
            "default": false
        },

        "part_name" : {
            "type": "string",
            "default": "_placeholder"
        },

		"part_options" : {
			"type": "object",
			"default": {}
		},

        "template_meta" : {
            "type": "object",
            "default": {}
        }
    },

	"example": {
        "attributes": {
            "preview": true
        }
    },
	"textdomain": "pds-blocks",
	"editorScript": "theme-part"
}