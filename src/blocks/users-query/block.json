{
	"apiVersion": 2,
	"name": "phenix/users-query",
	"version": "1.0.0",
	"title": "Users Query",
	"category": "phenix-blocks",
	"description": "Custom Query Posts Block.",
	"supports": {
		"html": false,
        "renaming": true,
        "align": ["full"],
        "alignWide": true,
        "__experimentalOnEnter": true,
		"__experimentalMetadata": true,
        "interactivity": {"clientNavigation": true}
	},

    "attributes" : {
        "metadata": {
            "type": "object",
            "default": {}
        },

        "id": {
            "type": "string",
            "default": ""
        },
        
        "query": {
			"type": "object",
            "default": {}
		},

        "template_part": {
            "type": "string",
            "default": "_placeholder"
        },

		"isFlexbox" : {
            "type": "boolean",
            "default": false
        },

        "flexbox": {
            "type": "object",
            "default": {}
        },

        "typography": {
            "type": "object",
            "default": {}
        },

        "style": {
            "type": "object",
            "default": {}
        },

        "responsive": {
            "type": "object",
            "default": {}
        },

        "slider" : {
            "type": "object",
            "default": {}
        },

		"preview" : {
			"type": "boolean",
			"default": false
		}
	},

	"example": {
        "attributes": {
            "preview": true
        }
    },

	"textdomain": "pds-blocks",
	"editorScript": "px-query"
}